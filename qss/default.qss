


QPushButton {
    color: #666666;
    background-color: #ffffff;
    border-width: 0px;
    border-color: #888888;
    border-style: solid;
    padding: 10px;
    border-radius: 3px;
    outline: none;
   font:bold;
   font-size: 14px;
   font-family: "Arial";
   text-align:left;
}




QPushButton:pressed {
    background-color: #ffffff;
    padding-top: -15px;
    padding-bottom: -17px;
}




QPushButton:hover {

    padding-top: -15px;
    padding-bottom: -17px;
}


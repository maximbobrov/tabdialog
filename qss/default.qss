    font-size: 12px;
    font-family: "Arial";
}
QHeaderView::section:vertical {
    color: #fff;
    border-style: solid;
    background-color:  gray;
 }


QHeaderView::section:horizontal {
    color: #fff;
    border-style: solid;
    background-color: #888888;
 }
